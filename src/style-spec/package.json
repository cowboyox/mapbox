{
  "name": "@mapbox/mapbox-gl-style-spec",
  "description": "a specification for mapbox gl styles",
  "version": "14.4.0",
  "author": "Mapbox",
  "keywords": [
    "mapbox",
    "mapbox-gl",
    "mapbox-gl-js"
  ],
  "license": "SEE LICENSE IN LICENSE.txt",
  "main": "./dist/index.cjs",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.es.js"
    },
    "./": {
      "import": "./"
    }
  },
  "scripts": {
    "pretest": "npm run build",
    "test": "node ./test.js",
    "build": "../../node_modules/.bin/rollup -c && ../../node_modules/.bin/rollup -c --environment esm",
    "build-dts": "../../node_modules/.bin/dts-bundle-generator --no-banner --export-referenced-types=false -o ./dist/index.d.ts ./style-spec.ts",
    "prepublishOnly": "npm run build && npm run build-dts",
    "postpublish": "rm dist/index.cjs dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mapbox/mapbox-gl-js.git"
  },
  "bin": {
    "gl-style-migrate": "./bin/gl-style-migrate.js",
    "gl-style-validate": "./bin/gl-style-validate.js",
    "gl-style-format": "./bin/gl-style-format.js",
    "gl-style-composite": "./bin/gl-style-composite.js"
  },
  "dependencies": {
    "@mapbox/jsonlint-lines-primitives": "~2.0.2",
    "@mapbox/point-geometry": "^0.1.0",
    "@mapbox/unitbezier": "^0.0.0",
    "cheap-ruler": "^3.0.1",
    "csscolorparser": "~1.0.2",
    "json-stringify-pretty-compact": "^2.0.0",
    "minimist": "^1.2.6",
    "quickselect": "^2.0.0",
    "rw": "^1.3.3",
    "sort-object": "^0.3.2",
    "tinyqueue": "^2.0.3"
  },
  "sideEffects": false
}
